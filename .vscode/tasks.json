{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "vite: dev",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "server: dev",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"options": {
				"cwd": "${workspaceFolder}/server"
			},
			"isBackground": true,
			"group": "build",
			"problemMatcher": []
