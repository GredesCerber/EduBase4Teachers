		{
			"label": "dev: all",
			"dependsOn": [
				"server: dev",
				"vite: dev"
			],
			"dependsOrder": "parallel"
		},
		{
			"label": "vite: dev",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"group": "build"
		},
		{
			"label": "build",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"build"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint-compact"
			],
			"group": "build"
		}
	]
